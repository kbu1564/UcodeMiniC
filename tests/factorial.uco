main       proc 2 2 1
           sym 2 1 1
           sym 2 2 1
           ldp
           lda 2 1
           call read
           ldp
           lod 2 1
           call write
           lod 2 1
           call factorial
           str 2 2
           ldp
           lod 2 2
           call write
           end
factorial  proc 2 2 2
           lod 2 1
           ldc 1
           eq
           fjp $$1
           retv
           end
$$1        lod 2 1
           ldc 1
           sub
           call factorial
           lod 2 1
           mult
           retv
           end
           bgn 0
           ldp
           call main
           end